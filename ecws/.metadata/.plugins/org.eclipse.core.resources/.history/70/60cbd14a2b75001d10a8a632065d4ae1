
public class PPSMain2 {

	public static void main(String[] args) {
		int 거스름돈 = 73090;
		int 돈=50000;
		
		int[] money = {50000, 10000, 5000, 1000, 500, 100, 50, 10};
		
		for(int i = 0; i<=money.length-1; i++) {
			if(돈 == money[i]) {
				int 몇개 = 거스름돈 / 돈;
				System.out.printf("%d원짜리 %d개\n", 돈, 몇개);
				거스름돈 %= 돈;				
			}
		}
		
	}
}
