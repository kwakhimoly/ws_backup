		try {
			String url = "jdbc:oracle:thin:@192.168.0.32:1521:xe";
			con = DriverManager.getConnection(url, "teacher", "1");
			
			String sql = "select * from dec22_jijum "
					+ "order by j_name";
			
		} catch (Exception e) {
			e.printStackTrace();
		}
		try {pstmt.close();} catch (Exception e) {}
