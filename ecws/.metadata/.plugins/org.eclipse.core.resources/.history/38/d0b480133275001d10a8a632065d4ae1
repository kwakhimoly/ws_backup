import java.util.Random;

//	up down 게임
//	컴퓨터가 1~10000 사이의 랜덤한 숫자 생각해놓게 함
//	내가 맞추기

//	뭐: 1212
//	up
//	---------
//	뭐: 7000
//	down
//	---------
//	...
//	뭐: 5043
//	5번 만에 정답


public class PPSMain3 {

	public static int getRandom() {
		Random r = new Random();
		int num = r.nextInt(1);
		if(num!=0) {
			num = r.nextInt(1);
		} 
		return num; 
	}
	
	public static void main(String[] args) {
		int number = getRandom();
		System.out.println(number);
	}
}
