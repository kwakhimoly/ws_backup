
public class RSMain2 {
	public static void main(String[] args) {
		for (int a = 2; a <= 9; a++) {
			for (int i = 1; i <= 9; i++) {
				System.out.printf("%d x %d = %d\n", a, i, a * i);
			}
			System.out.println("--------");
		}

		System.out.println("=====================");

		for (int b = 1; b <= 9; b++) {
			for (int a = 2; a <= 9; a++) {
				System.out.printf("%d x %d = %d\t", a, b, a * b);
			}
			System.out.println();
		}
	}
}
