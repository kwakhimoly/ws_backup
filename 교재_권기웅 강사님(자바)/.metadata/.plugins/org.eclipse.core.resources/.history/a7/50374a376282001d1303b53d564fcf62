package com.kwon.dec232mvc.main;

import java.util.Scanner;

public class ConsoleScreen {
	public static int getX() {
		Scanner k = new Scanner(System.in);
		System.out.print("x : ");
		int x = k.nextInt();
		//k.close(); // 닫아야, 다시는 못씀
		return x;
	}
	public static int getY() {
		Scanner k = new Scanner(System.in);
		System.out.print("y : ");
		int y = k.nextInt();
		k.close();
		return y;
	}
}
