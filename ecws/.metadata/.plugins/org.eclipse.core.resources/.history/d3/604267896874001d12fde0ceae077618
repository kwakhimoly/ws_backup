
public class RSMain2 {
	public static void main(String[] args) {
		for (int a = 2; a <= 3; a++) {
			for (int i = 1; i <= 9; i++) {
				System.out.printf("%d x %d = %d\n", a, i, a * i);
			}
			System.out.println("--------");
		}

		System.out.println("=====================");

		for (int i = 1; i <= 9; i++) {
			for (int a = 2; a <= 9; a++) {
				System.out.printf("%d x %d = %d\t", a, i, a * i);
			}
			System.out.println();
		}

		System.out.println("=====================");

		for (int i = 0; i < 5; i++) {
			for (int j = i; j < 5; j = i) {
				System.out.print("ã…‹");
			}
			System.out.println();
		}

	}
}
